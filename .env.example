# ============================================
# ENVIRONMENT CONFIGURATION TEMPLATE
# ============================================
# Copy this file to .env.development or .env.production
# and update the values according to your environment
#
# IMPORTANT: Never commit actual .env files with sensitive data to git!
# Only this .env.example file should be committed.

# API Configuration
VITE_API_BASE_URL=http://localhost:8080/api
VITE_API_TIMEOUT=30000

# App Configuration
VITE_APP_NAME=Coco App
VITE_APP_ENV=development

# Authentication
VITE_AUTH_TOKEN_KEY=coco_auth_token
VITE_AUTH_TOKEN_EXPIRY=3600

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG_MODE=true

# Add more environment variables as needed below
# VITE_SOME_OTHER_VAR=value
